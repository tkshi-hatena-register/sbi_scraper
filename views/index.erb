    <% num = 1 %>
    <% @first_brand.each do |row| %>
    <% if row["under"] == 0 %>
        <% next %>
      <% end %>
    <tr>
      <td align="left" width="24"><%= num %></td>
      <td width="300"><%= row["brand_name"] %></td>
      <td><%= row["code"] %>&nbsp;</td>
      <td width="59"><%= row["current_price"] %>&nbsp;</td>
      <td width="65"><%= row["over"] %></td>
      <td width="65"><%= row["under"] %></td>
      <td width="65"><%= row["time"] %></td>
      <td align="left" width="188"><%= row["under_divided_over"].round(1) %>倍</td>
    </tr>
    <% num += 1 %>
    <% end %>
  </tbody>
</table>
